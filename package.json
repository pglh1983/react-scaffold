  "repository": {
    "type": "git",
    "url": "git+https://.git"
  },
  "keywords": [],
  "pre-commit": [
    "lint"
  ],
  "scripts": {
    "build": "NODE_ENV=production parcel build src/index.html -d output/production/ --no-cache",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "start": "parcel src/index.html -d output/dev/",
